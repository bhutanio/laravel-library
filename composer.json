{
    "name": "bhutanio/laravel-utilities",
    "description": "Bhutan.io Laravel Utilities",
    "keywords": ["bhutanio", "laravel", "utilities"],
    "license": "MIT",
    "require": {
        "illuminate/contracts": "5.2.*",
        "illuminate/container": "5.2.*",
        "illuminate/database": "5.2.*",
        "illuminate/filesystem": "5.2.*",
        "illuminate/pagination": "5.2.*",
        "illuminate/console": "5.2.*",
        "guzzlehttp/guzzle": "6.*",
        "intervention/image": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload" : {
        "psr-4": {
            "Bhutanio\\": "src/Bhutanio/"
        },
        "files": [
            "src/Bhutanio/Laravel/Helpers.php"
        ]
    },
    "minimum-stability": "stable",
}
