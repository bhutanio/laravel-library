{
    "name": "bhutanio/laravel-utilities",
    "description": "Bhutan.io Laravel Utilities",
    "keywords": ["bhutanio", "laravel", "utilities"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.*",
        "guzzlehttp/guzzle": "6.*",
        "intervention/image": "2.*",
        "nexmo/client": "@beta"
    },
    "require-dev": {
        "mockery/mockery": "1.*",
        "phpunit/phpunit": "6.*"
    },
    "autoload" : {
        "psr-4": {
            "Bhutanio\\": "src/Bhutanio/"
        },
        "files": [
            "src/Bhutanio/Laravel/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bhutanio\\Laravel\\UtilitiesServiceProvider"
            ]
        }
    }
}
